{
    "maven": {
        "groupId": "${sapui5.groupId}",
        "artifactId": "${sapui5.artifactId}",
        "version": "${sapui5.version}",
        "classifier": "${sapui5.classifier}",
        "type": "${sapui5.type}"
    },
    "welcomeFile": "index.html",
    "authenticationMethod": "none",
    "routes": [
        {
            "path": "/",
            "target": {
              "type": "application",
              "account":"${account}",
              "name": "${sapui5.application}",
              "version": "${sapui5.version}",
              "preferLocal": true
            },
            "description": "SAPUI5 SDK (incl. CDN)"
        }
    ],
    "cacheControl": [{
        "maxAge": 604800
    }]
}
