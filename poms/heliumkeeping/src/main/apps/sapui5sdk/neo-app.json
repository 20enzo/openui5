{
    "maven": {
        "groupId": "${sapui5sdk.groupId}",
        "artifactId": "${sapui5sdk.artifactId}",
        "version": "${sapui5sdk.version}",
        "classifier": "${sapui5sdk.classifier}",
        "type": "${sapui5sdk.type}"
    },
    "welcomeFile": "index.html",
    "routes": [
        {
            "path": "/resources",
            "target": {
              "type": "application",
              "account":"${account}",
              "name": "${sapui5.application}",
              "entryPath": "/resources",
              "preferLocal": true,
              "version": "${sapui5.version}"
            },
            "description": "SAPUI5 HTML5 Application"
        }
    ],
    "cacheControl": [{
        "maxAge": 604800
    }],
    "ui5CacheBuster": true
}
