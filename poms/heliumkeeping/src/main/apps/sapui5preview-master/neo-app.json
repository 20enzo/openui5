{
	"maven": {
		"groupId": "${deployable.groupId}",
		"artifactId": "${deployable.artifactId}",
		"version": "${deployable.version}",
		"classifier": "${deployable.classifier}",
		"type": "${deployable.type}"
	},
	"welcomeFile": "index.html",
	"authenticationMethod": "none",
	"routes": [{
		"path": "/",
		"target": {
			"type": "destination",
			"name": "${destination}",
			"resolutionType": "localAccount",
			"preferLocal": true
		},
		"description": "SAPUI5 SDK (incl. CDN) - Version Alias: ${versionAlias} / Destination: ${destination}"
	}]
}
