<core:View xmlns:core="sap.ui.core"
	xmlns="sap.m" controllerName="BaseFioriApplication.view.Detail">
	<Page class="sapUiFioriObjectPage" title="{i18n>DETAIL_TITLE}"> 
		<content>
			<ObjectHeader title="{CustomerName}"
						number="{SalesOrderNumber}"
						numberUnit="{Currency}"></ObjectHeader>
			<Label text="{i18n>DETAIL_TEXT}"></Label>
		</content>
	</Page>
</core:View>