<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	

	<!--**************************************************************************
	* POM SECTION: Maven Model Version Settings
	***************************************************************************-->

	<modelVersion>4.0.0</modelVersion>
	
	
	<!--**************************************************************************
	* POM SECTION: Parent POM Settings
	***************************************************************************-->

	<parent>
		<groupId>com.sap.ui5.dist</groupId>
		<artifactId>reactor</artifactId>
		<version>1.22.9-SNAPSHOT</version>
		<relativePath>../pom.xml</relativePath>
	</parent>


	<!--**************************************************************************
	* POM SECTION: Maven Settings
	***************************************************************************-->

	<artifactId>uilib-collection</artifactId>
	<packaging>pom</packaging>


	<!--**************************************************************************
	* POM SECTION: Project Settings
	***************************************************************************-->

	<name>${project.artifactId}</name>
	<description>SAPUI5 Distribution - Libraries Collection</description>
	
	
	<!--**************************************************************************
	* POM SECTION: Properties Settings
	***************************************************************************-->

	<properties>
		<com.sap.suite.version>[1.22.8, 1.23.0-SNAPSHOT)</com.sap.suite.version>
		<com.sap.portal.ui5.version>[1.22.3, 1.23.0-SNAPSHOT)</com.sap.portal.ui5.version>
		<com.sap.ushell.version>[1.22.6, 1.23.0-SNAPSHOT)</com.sap.ushell.version>
		<com.sap.ui5.vbm.version>[1.22.6, 1.23.0-SNAPSHOT)</com.sap.ui5.vbm.version>
		<com.sap.collaboration.version>[1.22.4, 1.23.0-SNAPSHOT)</com.sap.collaboration.version>
		<com.sap.ca.version>[1.22.3, 1.23.0-SNAPSHOT)</com.sap.ca.version>
		<com.sap.apf.version>[1.22.3, 1.23.0-SNAPSHOT)</com.sap.apf.version>
		<com.sap.smartbusiness.version>[1.22.4, 1.23.0-SNAPSHOT)</com.sap.smartbusiness.version>
	</properties>


	<!--**************************************************************************
	* POM SECTION: Dependency Management Settings
	***************************************************************************-->

	<dependencyManagement>
	
		<dependencies>

			<!-- 
				Add references to all UI5 libraries here which should be part of the SAPUI5 distribution
			-->

			<!--
				UI5 Core Libraries	
				-->
			<dependency>
				<groupId>com.sap.ui5</groupId>
				<artifactId>uilib-collection</artifactId>
				<version>${sapui5.core.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>

			<!--
				Suite Commons
				-->
			<dependency>
				<groupId>com.sap.suite</groupId>
				<artifactId>suite-ui-commons</artifactId>
				<version>${com.sap.suite.version}</version>
			</dependency>

			<!--
				Unified Client
				-->
			<dependency>
				<groupId>com.sap.portal.ui5</groupId>
				<artifactId>portal-ui5-ext</artifactId>
				<version>${com.sap.portal.ui5.version}</version>
			</dependency>

			<!--
				Unified Shell 
				-->
			<dependency>
				<groupId>com.sap.ushell</groupId>
				<artifactId>ushell-lib</artifactId>
				<version>${com.sap.ushell.version}</version>
			</dependency>
			<!-- necessary for OSGi delivery incl. Launch Pad testenv -->
			<dependency>
				<groupId>com.sap.ushell</groupId>
				<artifactId>ushell-lib</artifactId>
				<version>${com.sap.ushell.version}</version>
				<classifier>testresources</classifier>
			</dependency>
			
			<!--
				Visual Business 
				-->
			<dependency>
				<groupId>com.sap.ui5</groupId>
				<artifactId>vbm</artifactId>
				<version>${com.sap.ui5.vbm.version}</version>
			</dependency>
			
			<!--
				Business Collaboration
				-->
			<dependency>
				<groupId>com.sap.collaboration</groupId>
				<artifactId>collaboration-commons</artifactId>
				<version>${com.sap.collaboration.version}</version>
			</dependency>
		
			<!--
				Fiori: sap.ca.ui (https://git.wdf.sap.corp:50000/git/?p=Fiori/sap.ca.git)
				-->
			<dependency>
				<groupId>com.sap.ca</groupId>
				<artifactId>ui</artifactId>
				<version>${com.sap.ca.version}</version>
			</dependency>
			<dependency>
				<groupId>com.sap.ca</groupId>
				<artifactId>scfld.md</artifactId>
				<version>${com.sap.ca.version}</version>
			</dependency>

			<!--
				Analysis Path Framework
				-->
			<dependency>
				<groupId>com.sap.apf</groupId>
				<artifactId>apf-lib</artifactId>
				<version>${com.sap.apf.version}</version>
			</dependency>
			
			<!--
				Smart Business
				 -->
			<dependency>
				<groupId>com.sap.smartbusiness</groupId>
				<artifactId>suite.smartbusiness</artifactId>
				<version>${com.sap.smartbusiness.version}</version>
			</dependency>

		</dependencies>
		
	</dependencyManagement>
	
	
	<!--**************************************************************************
	* POM SECTION: Dependency Settings
	***************************************************************************-->

	<dependencies>

		<dependency>
			<groupId>com.sap.ui5</groupId>
			<artifactId>uilib-collection</artifactId>
			<version>${sapui5.core.version}</version>
			<type>pom</type>
		</dependency>
		
		<dependency>
			<groupId>com.sap.suite</groupId>
			<artifactId>suite-ui-commons</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.portal.ui5</groupId>
			<artifactId>portal-ui5-ext</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.ushell</groupId>
			<artifactId>ushell-lib</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.ui5</groupId>
			<artifactId>vbm</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.collaboration</groupId>
			<artifactId>collaboration-commons</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.ca</groupId>
			<artifactId>ui</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.ca</groupId>
			<artifactId>scfld.md</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.apf</groupId>
			<artifactId>apf-lib</artifactId>
		</dependency>
		<dependency>
			<groupId>com.sap.smartbusiness</groupId>
			<artifactId>suite.smartbusiness</artifactId>
		</dependency>

	</dependencies>


</project>
