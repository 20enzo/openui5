<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.ui.commons" controllerName="webimage.controllers.home"
xmlns:ux3="sap.ui.ux3" xmlns:html="http://www.w3.org/1999/xhtml">
	<ux3:Shell showLogoutButton="false" showFeederTool="false" fullHeightContent="true" designType="Crystal" showSearchTool="false" headerType="SlimNavigation"
	id="app_shell_container" worksetItemSelected="onWorkItemSelected">
		<ux3:worksetItems>
			<ux3:NavigationItem id="wi_jobs" key="jobs" text="Jobs"></ux3:NavigationItem>
			<ux3:NavigationItem id="wi_modules" key="modules" text="Modules"></ux3:NavigationItem>
			<ux3:NavigationItem id="wi_images" key="images" text="Images"></ux3:NavigationItem>
			<ux3:NavigationItem id="wi_config" visible="false" key="config" text="CONFIG"></ux3:NavigationItem>
		</ux3:worksetItems>
		<ux3:headerItems>
			<TextView class="app-title" text="{/appTitle}"></TextView>
		</ux3:headerItems>
		<ux3:content>
			<mvc:XMLView id="jobs" viewName="webimage.views.jobs"></mvc:XMLView>
		</ux3:content>
	</ux3:Shell>
</core:View>