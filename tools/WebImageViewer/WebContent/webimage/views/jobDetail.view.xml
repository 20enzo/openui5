<core:View controllerName="webimage.controllers.jobDetail" xmlns="sap.ui.commons" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:np="notepad" xmlns:table="sap.ui.table">
    <np:Panel>
        <np:header>
            <np:BreadThread id="brd_thd" select="handlePressBack"/>
        </np:header>
        <np:content>
            <TabStrip id="category" tabs="{categorys}">
                <tabs>
                    <Tab text="{categoryName}">
                        <TabStrip tabs="{modules}">
                            <tabs>
                                <Tab text="{moduleName}">
                                    <table:Table columnSelect="colHandle" rows="{controls}" selectionMode="None" visibleRowCount="{count}">
                                        <table:columns>
                                            <table:Column showSortMenuEntry="false" sortProperty="controlName" width="200px">
                                                <TextView design="Bold" text="Control Name"/>
                                                <table:template>
                                                    <TextView text="{controlName}"/>
                                                </table:template>
                                            </table:Column>
                                            <table:Column hAlign="Center" showSortMenuEntry="pass" sortProperty="pass" width="40px">
                                                <TextView design="Bold" text="Pass"/>
                                                <table:template>
                                                    <TextView textAlign="Center" text="{pass}"/>
                                                </table:template>
                                            </table:Column>
                                            <table:Column hAlign="Center" showSortMenuEntry="false" sortProperty="fail" width="40px">
                                                <TextView design="Bold" text="Fail"/>
                                                <table:template>
                                                    <Link enabled="{path:'fail',formatter:'utils.formatter.Enable'}" press="displayFailImages" text="{fail}"/>
                                                </table:template>
                                            </table:Column>
                                        </table:columns>
                                    </table:Table>
                                </Tab>
                            </tabs>
                        </TabStrip>
                    </Tab>
                </tabs>
            </TabStrip>
        </np:content>
    </np:Panel>
</core:View>