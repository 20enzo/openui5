<core:View xmlns:core="sap.ui.core" xmlns="sap.ui.commons" controllerName="webimage.controllers.moduleDetail"
xmlns:ux3="sap.ui.ux3" xmlns:layout="sap.ui.commons.layout" xmlns:np="notepad"
xmlns:table="sap.ui.table" xmlns:vtypes="sap.viz.ui5.types" xmlns:html="http://www.w3.org/1999/xhtml">
	<np:Panel>
		<np:header>
			<np:BreadThread id="brd_thd" select="handlePressBack"></np:BreadThread>
		</np:header>
		<TabStrip id="tab_strip" tabs="{modules}">
			<tabs>
				<Tab text="{moduleName}">
					<layout:MatrixLayout>
						<layout:MatrixLayoutRow>
							<layout:MatrixLayoutCell>
								<table:Table id="resultTable" visibleRowCount="{count}" rows="{controls}" columnSelect="colHandle" selectionMode="None">
									<table:columns>
										<table:Column showSortMenuEntry="false" sortProperty="controlName" hAlign="Center">
											<TextView design="Bold" text="Name"/>
											<table:template>
												<TextView text="{controlName}"/>
											</table:template>
										</table:Column>
										<table:Column showSortMenuEntry="false" sortProperty="fail" hAlign="Center">
											<TextView design="Bold" text="Fail"/>
											<table:template>
												<Link text="{fail}" press="displayCtrlResults" enabled="{path:'fail',formatter:'.formatEnable'}"></Link>
											</table:template>
										</table:Column>
									</table:columns>
								</table:Table>
							</layout:MatrixLayoutCell>
						</layout:MatrixLayoutRow>
					</layout:MatrixLayout>
				</Tab>
			</tabs>
		</TabStrip>
	</np:Panel>
</core:View>
