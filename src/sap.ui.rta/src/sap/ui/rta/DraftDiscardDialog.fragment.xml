<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:core="sap.ui.core" >
	<Dialog
			title="{i18n>TIT_DRAFT_DISCARD_DIALOG}"
			state="Warning" >
		<content>
			<Text
					text="{i18n>TXT_DRAFT_DISCARD_DIALOG}"
					class="sapUiSmallMargin" />
		</content>
		<buttons>
			<Button
					id="sapUiRta_confirmSaveChangesOnVersionSwitchButton"
					text="{i18n>BUT_DRAFT_DISCARD_DIALOG_CONFIRM}"
					type="Emphasized"
					press="._discardDraftConfirmed" />
			<Button
					id="sapUiRta_cancelSaveChangesOnVersionSwitchButton"
					text="{i18n>BUT_DRAFT_DISCARD_DIALOG_CANCEL}"
					press="._discardDraftCanceled" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>